!!!
%html
  %head
    %meta{:charset => "UTF-8"}/
    = yield_content :head
    %link{:href => "https://fonts.googleapis.com/css?family=Ek+Mukta:400,500,600,700", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "https://fonts.googleapis.com/css?family=Lora:400,700", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "https://cdnjs.cloudflare.com/ajax/libs/jquery.swipebox/1.4.1/css/swipebox.min.css", :rel => "stylesheet", :type => "text/css"}/
    = stylesheet_link_tag "reset", "photos"
    %script{:src => "https://code.jquery.com/jquery-1.11.3.min.js", :type => "text/javascript"}
    %script{:src => "https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.25/browser-polyfill.min.js", :type => "text/javascript"}
    %script{:src => "https://cdnjs.cloudflare.com/ajax/libs/jquery.swipebox/1.4.1/js/jquery.swipebox.min.js", :type => "text/javascript"}
    %script{:src => "https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.25/browser.min.js", :type => "text/javascript"}
    %script{:src => "https://cdnjs.cloudflare.com/ajax/libs/jquery.imagesloaded/3.1.8/imagesloaded.pkgd.min.js", :type => "text/javascript"}
    %script{:src => "/javascripts/photos.js", :type => "text/babel"}
    = partial "google_analytics"
  %body
    = yield
